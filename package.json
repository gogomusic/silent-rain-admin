{
  "name": "silent-rain-admin",
  "version": "1.0.0",
  "private": true,
  "description": "「静夜聆雨」管理后台",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "dev": "pnpm start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "pnpm lint:js && pnpm lint:prettier && pnpm tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "openapi": "max openapi",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "pnpm build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "jest",
    "test:coverage": "pnpm jest -- --coverage",
    "test:update": "pnpm jest -- -u",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "pnpm lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/pro-components": "^2.8.10",
    "@umijs/route-utils": "^2.2.2",
    "ahooks": "^3.9.5",
    "antd": "^5.27.4",
    "antd-style": "^3.7.1",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-rsa": "^1.1.1",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.16.1",
    "rc-util": "^5.44.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.3.0",
    "@babel/core": "^7.28.4",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.23",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node-rsa": "^1.1.4",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet": "^6.1.11",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.5.1",
    "@umijs/max": "^4.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "express": "^4.21.2",
    "globals": "^16.4.0",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "react-dev-inspector": "^1.9.0",
    "stylelint": "^14.16.1",
    "swagger-ui-dist": "^4.19.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "stylelint",
        "@babel/core",
        "dva-core"
      ],
      "allowedVersions": {
        "react": "18",
        "react-dom": "18",
        "dva-core": "2"
      }
    }
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10"
  }
}
